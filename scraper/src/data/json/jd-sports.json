{
  "name": "JD Sports",
  "scrapingData": {
    "urls": [
      {
        "gender": "Man",
        "url": "https://www.jdsports.es/hombre/calzado-de-hombre/zapatillas"
      },
      {
        "gender": "Woman",
        "url": "https://www.jdsports.es/mujer/calzado-de-mujer/zapatillas"
      }
